{
  "name": "savepoint",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 6060 --turbopack",
    "postinstall": "prisma generate",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "preview": "next build && next start -p 6060",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "knip": "knip",
    "lint-ui": "lint-staged",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --config vitest.coverage.config.ts --coverage",
    "test:components": "vitest run --project components",
    "test:backend": "vitest run --project backend",
    "test:utilities": "vitest run --project utilities",
    "test:integration": "vitest run --project integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "db:cleanup-test": "docker exec savepoint-postgres psql -U postgres -t -c \"SELECT datname FROM pg_database WHERE datname LIKE 'test_%';\" | xargs -I {} docker exec savepoint-postgres dropdb -U postgres --if-exists \"{}\"",
    "ci:fix": "pnpm format:write && pnpm lint:fix",
    "ci:check": "pnpm lint && pnpm format:check && pnpm typecheck"
  },
